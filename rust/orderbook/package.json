{
    "name": "orderbook-native",
    "version": "0.1.0",
    "description": "High-performance order book implementation",
    "type": "module",
    "main": "native/index.node",
    "exports": {
        ".": {
            "import": "./native/index.node",
            "require": "./native/index.node"
        }
    },
    "scripts": {
        "build": "cd ../orderbook-bindings && neon build --release && cp native/index.node ../orderbook/native/",
        "build-debug": "cd ../orderbook-bindings && neon build && cp native/index.node ../orderbook/native/",
        "test": "cd ../orderbook-core && cargo test",
        "clean": "cd ../orderbook-bindings && cargo clean && rm -rf ../orderbook/native/"
    },
    "keywords": [
        "orderbook",
        "trading",
        "rust",
        "performance"
    ],
    "author": "OrderFlow Trading",
    "license": "MIT",
    "engines": {
        "node": ">=16.0.0"
    }
}
}
