{
    "name": "financial-math-native",
    "version": "0.1.0",
    "description": "High-performance financial mathematics using Rust u128 fixed-point arithmetic",
    "type": "module",
    "main": "native/index.node",
    "exports": {
        ".": {
            "import": "./native/index.node",
            "require": "./native/index.node"
        }
    },
    "scripts": {
        "build": "cd ../financial-math-bindings && neon build --release && cp native/index.node ../financial-math/native/",
        "build-debug": "cd ../financial-math-bindings && neon build && cp native/index.node ../financial-math/native/",
        "test": "cd ../financial-math && cargo test",
        "clean": "cd ../financial-math-bindings && cargo clean && rm -rf ../financial-math/native/"
    },
    "keywords": [
        "financial",
        "math",
        "rust",
        "fixed-point",
        "precision",
        "trading"
    ],
    "author": "Cryptology",
    "license": "MIT",
    "engines": {
        "node": ">=16.0.0"
    }
}
